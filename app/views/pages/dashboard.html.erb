
<div class="dashboard-page-container", id="dashboard">
  <!-- Sidebar -->
  <div class="w3-sidebar w3-bar-block w3-padding sidebar-bg w3-large pr-none sidebar" style="width:20%">
    
    <div class="w3-bar-item mb-3 mt-4">
        <%= link_to "My info", profile_path, class: "w3-bar-item w3-button hover-sidebar-list mb-4" %>
    <div>
    <div class="w3-bar-item mb-3 mt-4">
        <%= link_to "Progress photos", user_progresses_path(current_user), class: "w3-bar-item w3-button hover-sidebar-list mb-4" %>
    </div>
    <div class="w3-bar-item mb-3 mt-4">
        <%= link_to "Community", root_path, class: "w3-bar-item w3-button hover-sidebar-list mb-4" %>
    </div>
    <div class="w3-bar-item mb-3 mt-4">
        <%= link_to "Nutritional tips", root_path, class: "w3-bar-item w3-button hover-sidebar-list mb-4" %>
    </div>
    <div class="w3-bar-item mb-3 mt-4">
        <%= link_to "Contact", root_path, class: "w3-bar-item w3-button hover-sidebar-list mb-4" %>
    </div>
    <div class="w3-bar-item mb-3 mt-4">
        <%= link_to "Home", root_path, class: "back-btn w3-bar-item w3-button hover-sidebar-list mb-4" %>
    </div>
  </div>
 </div>
</div>


 <div class="page">
    <h2 class="header-page"> 12 week gym program </h2>
    <% Week.all.each do |week| %>
        <%= link_to week_path(week.id), class: "week-text" do  %>
            <div class="week">
                Week <%= week.week_number %>
            </div>
        <% end %>
    <% end %>
  </div>
 </div>


